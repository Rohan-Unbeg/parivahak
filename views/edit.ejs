<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>Sampark</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
      crossorigin="anonymous"
    />
    <link rel="stylesheet" href="/stylesheets/profile.css" />
    <link
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css"
      rel="stylesheet"
    />
  </head>

  <body>
    <!-- Navbar -->
    <nav class="navbar navbar-expand-lg bg-light navbar-light fixed-top mb-5">
      <div class="container">
        <a class="navbar-brand" href="/">PariVahak</a>
        <button
          class="navbar-toggler"
          type="button"
          data-bs-toggle="collapse"
          data-bs-target="#navbarSupportedContent"
          aria-controls="navbarSupportedContent"
          aria-expanded="false"
          aria-label="Toggle navigation"
        >
          <span class="navbar-toggler-icon"></span>
        </button>
        <div
          class="collapse navbar-collapse justify-content-end"
          id="navbarSupportedContent"
        >
          <ul class="navbar-nav mb-2 mb-lg-0">
            <li class="nav-item">
              <a class="nav-link" href="/edit" onclick="markActive(this)"
                >Profile</a
              >
            </li>
            <li class="nav-item">
              <a
                class="nav-link active"
                aria-current="page"
                href="#services"
                onclick="markActive(this)"
                >Services</a
              >
            </li>
            <li class="nav-item">
              <a
                class="nav-link"
                href="#about-contact"
                onclick="markActive(this)"
                >Contact</a
              >
            </li>
            <a type="button" href="/options" class="btn btn-primary ms-2"
              >Sign In</a
            >
          </ul>
        </div>
      </div>
    </nav>
    <!-- Navbar -->

  
<!-- main   -->

<!-- main   -->

    <footer class="footer mt-5 py-3">
      <div class="container text-center">
        <span>Â© 2024 Sampark. All rights reserved.</span>
      </div>
    </footer>
    <!-- Footer -->

    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
      crossorigin="anonymous"
    ></script>

    <script>
      function markActive(link) {
        var links = document.querySelectorAll(".nav-link");
        links.forEach(function (item) {
          item.classList.remove("active");
        });
        link.classList.add("active");
      }
    </script>
  </body>
</html>
